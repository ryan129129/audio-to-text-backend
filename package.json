    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-fastify": "^11.1.9",
    "@supabase/supabase-js": "^2.89.0",
    "bullmq": "^5.66.1",
